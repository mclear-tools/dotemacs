(unless (>= emacs-major-version 24)
  (error "omtose phellack requires Emacs 24 or later!"))

(deftheme omtose-darker "Dark and soothing colorscheme, with cool bluish touch")

(defgroup omtose-darker nil
  "omtose-darker options."
  :group 'faces)

